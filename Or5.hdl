/* chip to output the OR of five input bits at once */

CHIP Or5{
    IN x1, x2, x3, x4, x5;
    OUT out;

    PARTS:
    Or(a=x1, b=x2, out=y1);
    Or(a=y1, b=x3, out=y2);
    Or(a=y2, b=x4, out=y3);
    Or(a=y3, b=x5, out=out);
}