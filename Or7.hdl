/* evaluates Or of 7 operands at once */

CHIP Or7{
    IN x1, x2, x3, x4, x5,x6,x7;
    OUT out;

    PARTS:
    Or(a=x1, b=x2, out=y1);
    Or(a=y1, b=x3, out=y2);
    Or(a=y2, b=x4, out=y3);
    Or(a=y3, b=x5, out=y4);
    Or(a=y4, b=x6, out=y5);
    Or(a=y5, b=x7, out=out);
}